{% for m in nut_upsmon_monitors %}
MONITOR {{ m.upsname | default(m.name) }}@{{ m.upsmon_system }} {{ m.power_value }} {{ m.username }} {{ m.password }} {{ m.type }}
{% endfor %}

MINSUPPLIES {{ nut_upsmon_minsupplies }}

SHUTDOWNCMD "{{ nut_upsmon_shutdowncmd }}"

{% if nut_upsmon_notifycmd %}
NOTIFYCMD "{{ nut_upsmon_notifycmd }}"
{% endif %}

POLLFREQ {{ nut_upsmon_pollinterval }}
POLLFREQALERT {{ nut_upsmon_pollinterval }}
